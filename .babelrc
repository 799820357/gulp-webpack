{
  "presets": [
    [
      "@babel/preset-env", 
      {
		"modules": "umd",
        "targets": {
          "browsers": ["> 1%", "last 2 versions", "not ie <= 9"]
        },
        "corejs":{ "version": 3, "proposals": true },
        "useBuiltIns" : "usage"
      }
    ],
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
	"@babel/plugin-proposal-class-properties"
  ]
}