{
  "presets": [
    [
      "@babel/preset-env", 
      {
        "targets": {
          "browsers": ["> 1%", "last 2 versions", "ie >= 9"]
        },
        "corejs":{ "version": 3, "proposals": true },
        "useBuiltIns" : "usage"
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": 3
      }
    ]
  ]
}